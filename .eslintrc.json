{
  "root": true,
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": { "ecmaVersion": 12 },
  "rules": {
    "camelcase": 0,
    "comma-dangle": 0,
    "linebreak-style": 0,
    "import/extensions": [ 2, "ignorePackages" ]
  }
}